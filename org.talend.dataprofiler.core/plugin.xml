               </adapt>
         </enablement>
      </actionProvider>
      <actionProvider
            class="org.talend.dataprofiler.core.ui.action.provider.DeleteUserFolderProvider"
            id="org.talend.dataprofiler.core.ui.action.provider.DeleteUserFolderProvider">
         <enablement>
               <adapt
                     type="org.eclipse.core.resources.IFolder">
               </adapt>
         </enablement>
      </actionProvider>
      <actionProvider
            class="org.talend.dataprofiler.core.ui.action.provider.predefined.CreateNominalAnalysis"
