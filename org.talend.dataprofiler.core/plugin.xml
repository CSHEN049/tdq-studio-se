      <actionProvider
            class="org.talend.dataprofiler.core.ui.action.provider.predefined.CreateSummaryStatisticsAnalysis"
            id="org.talend.dataprofiler.core.ui.action.provider.predefined.CreateSummaryStatisticsAnalysis">
