               </instanceof>
            </and>
         </enablement>
      </actionProvider>
      <actionProvider
            class="org.talend.dataprofiler.core.ui.action.provider.predefined.CreateDateAnalysis"
            id="org.talend.dataprofiler.core.ui.action.provider.predefined.CreateDateAnalysis">
