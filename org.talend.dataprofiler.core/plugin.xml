            <and>
               <instanceof
                     value="org.talend.cwm.relational.TdColumn">
               </instanceof>
            </and>
         </enablement>
      </actionProvider>
      <actionProvider
            class="org.talend.dataprofiler.core.ui.action.provider.predefined.CreateDiscreteAnalysis"
            id="org.talend.dataprofiler.core.ui.action.provider.predefined.CreateDiscreteAnalysis">
